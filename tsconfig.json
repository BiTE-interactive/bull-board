{
  // "exclude": ["dist/**/*"],
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2019", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2019",
    "outDir": "./dist",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "declaration": true
  },
}
